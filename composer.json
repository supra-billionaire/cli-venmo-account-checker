{
  "name": "supracodes/cli-amex-account-checker",
  "description": "A command-line application for efficiently checking multiple American Express accounts.",
  "authors": [
    {
      "name": "Supra Billionaire",
      "email": "billionaire@supra.codes"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/fork": "^1.2"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/http": "^10.0",
    "laravel-zero/framework": "^10.0.2",
    "nunomaduro/termwind": "^1.15.1",
    "laravel/pint": "^1.8",
    "mockery/mockery": "^1.5.1",
    "pestphp/pest": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "builds/amex"
  ]
}
